@using chefNdish.Models;
@model List<Chef>

@{
    ViewData["Title"] = "Chefs";
}
<div class="container">

<h1>@ViewData["Title"]</h1>
    <table class="table">
        <thead>
            <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Birthday</th>
            </tr>
        </thead>
        <tbody>
        @foreach (Chef c in @Model)
        {
            <tr>
            <td>@c.FirstName</td>
            <td>@c.LastName</td>
            <td>@c.Birthday</td>
            </tr>
        }
    </table>
</div>
<a href="/new">Add A Chef!</a>